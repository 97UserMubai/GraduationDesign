<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="systemOa.dao.IFileBeanDao">

    <insert id="insertFileLog" >
        insert  into systemoa.file(fileId,filePerson,fileName,fileDepartment,fileTime,fileSource)
        values
        (#{fileId},#{filePerson},#{fileName},#{fileDepartment},#{fileTime},#{fileSource})
    </insert>

</mapper>