<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="systemOa.dao.IBusinessDao">

    <insert id="insertNewBusiness">
        insert into systemoa.business(businessId,userId,employeeId,userName,employeeName,
        businessName,businessProduceTime,businessStatus)
        values
        (#{businessId},#{userId},#{employeeId},#{userName},#{employeeName},
        #{businessName},#{businessProduceTime},#{businessStatus})
    </insert>

</mapper>